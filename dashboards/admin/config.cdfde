     {
      "name": "postExecution",
      "value": "function(){\n    //bissolCreateCheckboxSet('html_db_column_picker','dbColumnPicker', 'Choose Columns:', result_fetch_db_columns, 'param_db_column');\n    bissolCreateTableConfigPicker(result_fetch_db_columns);\n    \n    // enrich with existing config data if exists\n    if(result_fetch_config.length > 0){\n        \n        var column = $('#html_db_table_metadata_picker > table > tbody > tr');\n        \n        $.each(column,function(i, cell){\n            var colPositionPostion = $(this).find('td:nth-child(1)');\n            var colNameCell = $(this).find('td:nth-child(2)');\n            \n            //console.log('--------');\n            //console.log(colNameCell.text());\n            //console.log(param_col_names);\n            //console.log(param_col_names.indexOf(colNameCell.text()));\n            \n            if(param_col_names.indexOf(colNameCell.text())>-1){\n                colPositionPostion.prepend('<span class=\"label label-primary bissolConfigLabel\">Configured</span>');\n                //colNameCell.addClass('bissolExistsInConfig');\n            } else {\n                //colNameCell.addClass('bissolNotExistsInConfig');\n                colPositionPostion.prepend('<span class=\"label label-info bissolConfigLabel\">New</span>');\n            }\n            \n            if($.inArray(colNameCell.text(), param_col_names_visible) > -1){\n                $(this).find('td:nth-child(5) > input').attr('checked','');\n            }\n            \n            if($.inArray(colNameCell.text(), param_col_names_editable) > -1){\n                $(this).find('td:nth-child(6) > input').attr('checked','');\n            }\n            \n            if(colNameCell.text() === param_id_column){\n                $(this).find('td:nth-child(7) > input').attr('checked','');\n            }\n    \n        }); \n        \n        $('#html_db_table_metadata_picker').append(\n            '<button type=\"submit\" id=\"bissol-table-properties-delete\" class=\"btn btn-danger bissolConfigSubmit\">Delete</button>'\n            );\n            \n        bissolSaveAction();\n    }\n} ",
