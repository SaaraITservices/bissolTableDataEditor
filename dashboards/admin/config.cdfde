      "name": "dataSource",
      "value": "qry_fetch_config_tables",
      "type": "Datasource"
     },
     {
      "name": "refreshPeriod",
      "value": "",
      "type": "Integer"
     },
     {
      "name": "priority",
      "value": 5,
      "type": "Integer"
     },
     {
      "name": "htmlObject",
      "value": "${p:html_config_action_picker}",
      "type": "HtmlObject"
     },
     {
      "name": "executeAtStart",
      "value": "true",
      "type": "Boolean"
     },
     {
      "name": "preExecution",
      "value": "",
      "type": "JavaScript"
     },
     {
      "name": "postExecution",
      "value": "function(){\n    \n    if(result_fetch_config_tables.length > 0){\n        bissolCreateSelect(\n            {\n                myCdeContainerId: '#html_config_action_picker'\n                , myDashboardObjectId: 'configActionPicker'\n                , myLabelText: 'Choose Action:'\n                , myData: result_fetch_config_tables\n                , cdeParam: 'param_config_id'\n            }\n        );      \n    } else {\n        // in case no table was set up there will be no result so provide workaround\n        // very nasty workaround\n        var fakeSet = [];\n        fakeSet.push(\"1\");\n        bissolCreateSelect(\n            {\n                myCdeContainerId: '#html_config_action_picker'\n                , myDashboardObjectId: 'configActionPicker'\n                , myLabelText: 'Choose Action:'\n                , myData: fakeSet\n                , cdeParam: 'param_config_id'\n            }            \n        ); \n        $('#configActionPicker > option:nth-child(2)')\n        .attr('disabled','').text('--- NO TABLE CONFIGURED ---');\n    }\n    \n   \n    // add new element at the beginning\n    $('#configActionPicker').prepend(\n            '<option disabled selected>PICK AN ACTION</option>'\n            + '<option value=\"new-table-config\">Configure new table</option>' \n            );\n    // override descriptive text of the 2nd option\n    $('#configActionPicker > option:nth-child(3)').text('Or choose to edit a configured table below:');\n   \n}",
      "type": "JavaScript"
     },
     {
      "name": "postFetch",
      "value": "",
      "type": "JavaScript"
     },
     {
      "name": "preChange",
      "value": "",
      "type": "JavaScript"
     },
     {
      "name": "postChange",
      "value": "",
      "type": "JavaScript"
     },
     {
      "name": "tooltip",
      "value": "",
      "type": "Html"
     }
